{
  "name": "datocms-plugin-yandex-translate",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"yarn serve\" \"yarn webpack:dev\"",
    "serve": "serve -p 5000 dist",
    "dist": "NODE_ENV=production webpack --progress",
    "prepublish": "rimraf lib dist && mkdir dist && npm run dist",
    "webpack:dev": "NODE_ENV=development webpack --progress --watch",
    "test": "echo 1"
  },
  "keywords": [
    "datocms",
    "datocms-plugin"
  ],
  "author": "DatoCMS <support@datocms.com>",
  "license": "ISC",
  "datoCmsPlugin": {
    "title": "Yandex Translate",
    "description": "Makes it easier to automatically translate your content into secondary languages",
    "url": "https://unpkg.com/datocms-plugin-yandex-translate/dist/index.html",
    "pluginType": "field_addon",
    "fieldTypes": ["string", "text"],
    "parameters": {
      "global": [
        {
          "id": "developmentMode",
          "label": "Development mode?",
          "type": "boolean",
          "hint": "Enable development logs on the console"
        },
        {
          "id": "yandexApiKey",
          "label": "Yandex API Key",
          "type": "string",
          "required": true,
          "hint": "The Yandex API Key to use (see https://tech.yandex.com/translate/)",
          "default": "trnsl.1.1.20151015T080754Z.fac48f0d13a96c3a.c0c58058288c42ba40de8aec2b36d9d86c3adb1d"
        }
      ],
      "instance": []
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.54",
    "@babel/core": "^7.0.0-beta.54",
    "@babel/preset-env": "^7.0.0-beta.54",
    "@babel/register": "^7.0.0-beta.54",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^8.0.0-beta",
    "concurrently": "^4.0.1",
    "css-loader": "^1.0.0",
    "eslint": "^5.3.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-import": "^2.14.0",
    "html-webpack-include-assets-plugin": "^1.0.5",
    "html-webpack-inline-source-plugin": "^0.0.10",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.3",
    "node-sass": "^4.9.3",
    "np": "^3.0.4",
    "sass-loader": "^7.1.0",
    "serve": "^10.0.2",
    "style-loader": "^0.23.0",
    "webpack": "^4.16.1",
    "webpack-cli": "^3.1.0"
  },
  "dependencies": {
    "debounce": "^1.2.0",
    "eslint-import-resolver-webpack": "^0.10.1",
    "to-querystring": "^1.0.4"
  }
}
