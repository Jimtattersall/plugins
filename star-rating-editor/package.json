{
  "name": "datocms-plugin-star-rating-editor",
  "version": "0.0.2",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"yarn serve\" \"yarn webpack:dev\"",
    "serve": "serve -p 5000 dist",
    "dist": "NODE_ENV=production webpack --progress",
    "webpack:dev": "NODE_ENV=development webpack --progress --watch",
    "test": "echo 1"
  },
  "keywords": [
    "datocms",
    "datocms-plugin"
  ],
  "author": "DatoCMS <support@datocms.com>",
  "license": "ISC",
  "datoCmsPlugin": {
    "title": "Star rating editor",
    "description": "A plugin that presents integer fields as star rating widgets",
    "url": "https://unpkg.com/datocms-plugin-star-rating-editor/dist/index.html",
    "fieldTypes": [
      "integer"
    ],
    "pluginType": "field_editor",
    "parameters": {
      "global": [
        {
          "id": "developmentMode",
          "label": "Development mode?",
          "type": "boolean",
          "hint": "Enable development logs on the console"
        }
      ],
      "instance": [
        {
          "id": "maxRating",
          "label": "Maximum rating",
          "type": "integer",
          "required": true,
          "default": 5
        },
        {
          "id": "starsColor",
          "label": "Stars color",
          "type": "color",
          "required": true,
          "default": {
            "red": 255,
            "green": 180,
            "blue": 0,
            "alpha": 255
          }
        }
      ]
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.54",
    "@babel/core": "^7.0.0-beta.54",
    "@babel/preset-env": "^7.0.0-beta.54",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0-beta.54",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^8.0.0-beta",
    "concurrently": "^4.0.1",
    "css-loader": "^1.0.0",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.0",
    "html-webpack-include-assets-plugin": "^1.0.5",
    "html-webpack-inline-source-plugin": "^0.0.10",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.3",
    "node-sass": "^4.9.3",
    "np": "^3.0.4",
    "sass-loader": "^7.1.0",
    "serve": "^10.0.2",
    "webpack": "^4.16.1",
    "webpack-cli": "^3.1.0"
  },
  "dependencies": {
    "debounce": "^1.2.0",
    "eslint-import-resolver-webpack": "^0.10.1",
    "preact": "^8.3.1",
    "preact-compat": "^3.18.4",
    "react-star-rating-component": "^1.4.1",
    "rgb-hex": "^2.1.0",
    "to-querystring": "^1.0.4"
  }
}
