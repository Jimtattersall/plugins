{
  "name": "Tag editor",
  "description": "A plugin that transforms any string and JSON field into a tag editor",
  "url": "https://unpkg.com/datocms-plugin-tag-editor/dist/index.html",
  "fieldTypes": ["string", "json"],
  "pluginType": "field_editor",
  "parameters": {
    "global": [
      {
        "id": "developmentMode",
        "label": "Development mode?",
        "type": "boolean",
        "hint": "Enable development logs on the console"
      }
    ],
    "instance": [
    	{
      	"id": "maxStringsList",
        "label": "Maximum stringsList",
        "type": "integer",
        "required": true,
        "default": 5
      },
      {
        "id": "color",
        "label": "Star color",
        "type": "color"
      }
    ]
  }
}
