{
  "name": "datocms-plugin-tag-editor",
  "homepage": "https://github.com/datocms/plugins/tree/master/tag-editor#readme",
  "version": "0.0.3",
  "description": "A plugin that transforms any string and JSON field into a tag editor",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"yarn serve\" \"yarn webpack:dev\"",
    "serve": "serve -p 5000 dist",
    "dist": "NODE_ENV=production webpack --progress",
    "prepublish": "rimraf lib dist && mkdir dist && npm run dist",
    "webpack:dev": "NODE_ENV=development webpack --progress --watch",
    "test": "echo 1"
  },
  "keywords": [
    "datocms",
    "datocms-plugin",
    "field-editor",
    "string-field",
    "json-field",
    "tags"
  ],
  "author": "DatoCMS <support@datocms.com>",
  "license": "ISC",
  "datoCmsPlugin": {
    "title": "Tag editor",
    "entryPoint": "dist/index.html",
    "fieldTypes": [
      "string",
      "json"
    ],
    "pluginType": "field_editor",
    "parameters": {
      "global": [
        {
          "id": "developmentMode",
          "label": "Development mode?",
          "type": "boolean",
          "hint": "Enable development logs on the console"
        }
      ],
      "instance": []
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.54",
    "@babel/core": "^7.0.0-beta.54",
    "@babel/preset-env": "^7.0.0-beta.54",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0-beta.54",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^8.0.0-beta",
    "concurrently": "^4.0.1",
    "css-loader": "^1.0.0",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.0",
    "html-webpack-include-assets-plugin": "^1.0.5",
    "html-webpack-inline-source-plugin": "^0.0.10",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.3",
    "node-sass": "^4.9.3",
    "sass-loader": "^7.1.0",
    "serve": "^10.0.2",
    "webpack": "^4.16.1",
    "webpack-cli": "^3.1.0"
  },
  "dependencies": {
    "eslint-import-resolver-webpack": "^0.10.1",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-tag-input": "^5.2.3",
    "style-loader": "^0.23.0",
    "to-querystring": "^1.0.4"
  }
}
